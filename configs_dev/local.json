{
  "unityPath": "C:\\Program Files\\Unity-5.6.1p1",
  "projectPath": "./UnityProject_Minamo",

  "buildBasePath": "./output",
  "buildPath": "{{.ConfigName}}-{{.Year}}-{{.Month}}-{{.Day}}-{{.Platform}}/{{.ShortRevision}}.exe",

  "method": "Assets.Minamo.Editor.EntryPoint.Build",

  "build": {
    "targetGroup": "standalone",
    "target": "windows",

    "options": {
      "development": true,
      "allowDebugging": true
    }
  },

  "defines": [
    "HELLO_WORLD",
    "PLATFORM_WIN32_STEAMVR"
  ],

  "vrDevices": {
    "enabled": true,
    "devices": "OpenVR",
    "stereoRenderingPath": "single-pass"
  },

  "identification": {
    "packageName": "com.fiveminlab.minamo.localdev",
    "versionName": "0.0.1",
    "versionCode": 1
  },

  "androidSdk": {
    "min": 24,
    "target": 0
  },

  "keystore": {
    "keystoreName": "",
    "keystorePass": "",
    "keyaliasName": "",
    "keyaliasPass": ""
  },

  "publishing": {
    "useApkExpansion": true
  }
}
