{
  "unityPath": "/Applications/Unity/Unity.app",
  "projectPath": "./UnityProject_Minamo",

  "buildBasePath": "./output",
  "buildPath": "{{.ConfigName}}-{{.Year}}-{{.Month}}-{{.Day}}-{{.ShortRevision}}.ipa",

  "method": "Assets.Minamo.Editor.EntryPoint.Build",

  "build": {
    "targetGroup": "ios",
    "target": "ios",

    "options": {
      "development": true,
      "allowDebugging": true,
      "acceptExternalModificationsToPlayer": true
    }
  },

  "identification": {
    "packageName": "com.fiveminlab.minamo.ios",
    "versionName": "0.0.1",
    "versionCode": 1
  },
  
  "resolutionAndPresentation": {
    "runInBackground": true
  },

  "vrDevices": {
    "enabled": false,
    "devices": ""
  }
}
